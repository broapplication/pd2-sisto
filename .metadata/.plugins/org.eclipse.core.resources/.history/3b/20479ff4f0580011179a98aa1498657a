package it.polito.pd2.WF.sol6.client2;


public class Client2 {

	
	/**
	 * @param args
	 */
	public static void main(String[] args) {
				
		if(args.length!=2){
			System.err.println("usage: Client2 <ActorName> <ActorRole>");
			System.exit(1);
		}
		
		try {
			//System.setProperty("it.polito.pd2.WF.sol6.URL","http://localhost:8181/WorkflowService");
			cli=new Client2Imp(args[0], args[1]);
		} 
		catch (Client2Exception e) {
			e.printStackTrace();
			System.exit(1);
		}
		System.out.println("Terminated.");
	}
}
